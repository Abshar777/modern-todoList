{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "resolveJsonModule": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./backend",
      "noImplicitAny": true
    },
    "include": ["backend"],
    "exclude": ["node_modules"]
   }
  